#include <unistd.h>

int main() {
    int guess;
    int secret_number = 7; // NÃºmero secreto para ganar

    printf("Â¡Bienvenido al juego de adivinanzas!\n");
    printf("Adivina el nÃºmero secreto (entre 1 y 10): ");
    scanf("%d", &guess);

    if (guess == secret_number) {
        printf("Â¡Felicidades! Has adivinado el nÃºmero.\n");
